    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run franks-port-admin:serve-ssr",
    "serve:ssr": "node dist/franks-port-admin/server/main.js",
    "build:ssr": "ng build --prod && ng run franks-port-admin:server:production",
    "prerender": "ng run franks-port-admin:prerender",
    "postinstall": "ngcc"
