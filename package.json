  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "buildzip": "ng build --prod ; cd .. ; cd dist-zip/ ; zip -r admin.zip admin/ ; cd ../admin-frontend",
    "build": "npm i ; ng build --prod ; npm run deploy",
    "buildloc": "npm i ; ng build --prod ; npm run deployloc",
    "build:prod": "npm i ; ng build --prod ; npm run deploy:prod",
    "deployloc": "rm -rf /var/www/allow24.in/adev ; mkdir /var/www/allow24.in/adev ; scp -r /home/aura-exch-front/admin-frontend/dist/* /var/www/allow24.in/adev",
    "deploy": "ssh new-development -a rm -rf /var/www/allow24.in/adev/* ; scp -r dist/* new-development:/var/www/allow24.in/adev",
